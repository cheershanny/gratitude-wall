generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Message {
  id        Int      @id @default(autoincrement())
  message   String
  name      String?  @db.VarChar(255)
  createdAt DateTime @default(now())
}
